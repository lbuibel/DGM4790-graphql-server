datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:docker@localhost:5432/bike-route-docker?schema=public"
}

generator client {
  provider = "prisma-client-js"
}

model Route {
  id                String   @default(cuid()) @id
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt
  name              String
  type              String
  miles             Float
  startingElevation Int
  finalElevation    Int
  iframeData        String
}